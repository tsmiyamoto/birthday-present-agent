# Google Gemini API key (used by google-genai and ADK)
GOOGLE_API_KEY=

# Optional: override the default Gemini model for ADK
# GEMINI_MODEL=gemini-2.5-flash

# xAI Grok API information
GROK_API_KEY=
# GROK_MODEL=grok-2-latest
# GROK_API_URL=https://api.x.ai/v1/chat/completions

# SerpApi credentials for Google Shopping search
SERPAPI_API_KEY=
# SERPAPI_GL=jp
# SERPAPI_HL=ja
# SERPAPI_SHOPPING_ENDPOINT=https://serpapi.com/search.json
# SERPAPI_PRODUCT_ENDPOINT=https://serpapi.com/search.json

# Optional: customize the ADK application name shown in logs/UI
# ADK_APP_NAME=birthday-present-agent

# --- Vertex AI Agent Engine deployment ---
# Google Cloud project that hosts the Agent Engine deployment.
GOOGLE_CLOUD_PROJECT=

# Vertex AI region. Agent Engine is available in a limited set of regions (e.g. us-central1).
GOOGLE_CLOUD_LOCATION=us-central1

# Cloud Storage bucket used as the staging bucket for deployment artifacts. Either the
# bucket name ("my-staging-bucket") or a full URI ("gs://my-staging-bucket").
GOOGLE_CLOUD_STORAGE_BUCKET=

# Path to the service account JSON file with the required Vertex AI permissions.
GOOGLE_APPLICATION_CREDENTIALS=

# Optional: override the default Agent Engine endpoint if necessary.
# VERTEX_AI_AGENT_SERVICE_ENDPOINT=

# Agent Engine deployment identifier (projects/<project>/locations/<region>/reasoningEngines/<id>)
VERTEX_AI_AGENT_ENGINE_ID=
